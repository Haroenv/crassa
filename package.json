{
  "name": "crassa",
  "version": "0.3.6",
  "preferGlobal": true,
  "bin": "./index.js",
  "main": "index.js",
  "author": "Ghondar <abgv9221@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/ghondar/crassa.git"
  },
  "homepage": "https://github.com/ghondar/crassa",
  "keywords": [
    "react",
    "reactjs",
    "express",
    "redux",
    "create-react-app",
    "cli",
    "universal",
    "server-side-application"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.1.2",
    "@babel/polyfill": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-cli": "6.26.0",
    "babel-plugin-dynamic-import-node-babel-7": "2.0.7",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "body-parser": "1.18.3",
    "commander": "2.19.0",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "customize-cra": "0.1.1",
    "download-git-repo": "1.1.0",
    "express": "4.16.4",
    "http-proxy-middleware": "0.19.0",
    "ignore-styles": "5.0.1",
    "immer": "1.7.4",
    "inquirer": "6.2.0",
    "lint-staged": "8.0.3",
    "loadable-components": "2.2.3",
    "module-alias": "2.1.0",
    "morgan": "1.9.1",
    "ncp": "2.0.0",
    "nodemon": "1.18.5",
    "pre-commit": "1.2.2",
    "react-app-rewired": "1.6.2",
    "react-scripts": "2.0.5",
    "rimraf": "2.6.2"
  },
  "scripts": {
    "publish": "git add . && git commit && npm publish",
    "local-pack": "npm pack && rm -rf ../crassa-no-git && mkdir ../crassa-no-git && mv *.tgz ../crassa-no-git && cd ../crassa-no-git && tar -xvf *.tgz && cp -rf package/. . && rm -rf package/ && rm -rf *.tgz && npm i && sudo npm link",
    "format": "eslint . --ext .js --fix",
    "lint-staged": "lint-staged"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "{src,server}/**/*.js": [
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "_moduleAliases": {
    "../config/paths": "./react-scripts-wrapper-paths",
    "./paths": "./react-scripts-wrapper-paths"
  }
}
